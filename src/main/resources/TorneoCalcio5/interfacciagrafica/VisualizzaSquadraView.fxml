<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="450.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TorneoCalcio5.interfacciagrafica.VisualizzaSquadraController">
   <children>
      <VBox layoutX="20.0" layoutY="20.0" spacing="20.0" AnchorPane.bottomAnchor="20.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="20.0">
         <children>
            <Label fx:id="labelNomeSquadra" text=" Squadra: [Nome]">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
            </Label>
            <TableView fx:id="tabellaGiocatori" VBox.vgrow="ALWAYS">
              <columns>
                <TableColumn fx:id="colNome" prefWidth="150.0" text="Nome" />
                <TableColumn fx:id="colCognome" prefWidth="150.0" text="Cognome" />
                  <TableColumn fx:id="colDataNascita" prefWidth="150.0" text="Data di Nascita" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <HBox alignment="CENTER_RIGHT" spacing="10.0">
               <children>
                  <Button mnemonicParsing="false" text="Modifica" />
                  <Button mnemonicParsing="false" onAction="#chiudiFinestra" text="Chiudi" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
