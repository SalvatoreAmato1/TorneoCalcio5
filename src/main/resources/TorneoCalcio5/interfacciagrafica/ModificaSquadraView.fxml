<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="500.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" >
   <children>
      <VBox spacing="0.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="70.0" style="-fx-background-color: #2c3e50;">
               <children>
                  <Label textFill="WHITE">
                     <font> <Font name="System Bold" size="20.0" /> </font>
                  </Label>
               </children>
               <padding> <Insets left="20.0" /> </padding>
            </HBox>

            <VBox prefHeight="264.0" prefWidth="650.0" spacing="20.0" VBox.vgrow="ALWAYS">
               <children>
                  <TextField fx:id="nomeField" prefHeight="39.0" prefWidth="114.0" promptText="nome squadra" />
                  
                  <HBox spacing="10.0" style="-fx-background-color: #ecf0f1;">
                     <children>
                        <Label prefWidth="150.0" text="Nome" />
                        <Label prefWidth="150.0" text="Cognome" />
                        <Label prefWidth="150.0" text="Data di Nascita" />
                     </children>
                     <padding> <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" /> </padding>
                  </HBox>

                  <HBox spacing="10.0">
                     <children>
                        <TextField fx:id="txtNome1" prefWidth="150.0" promptText="Nome" />
                        <TextField fx:id="txtCognome1" prefWidth="150.0" promptText="Cognome" />
                        <DatePicker fx:id="dpData1" prefWidth="150.0" />
                     </children>
                  </HBox>
                  <HBox layoutX="30.0" layoutY="124.0" spacing="10.0">
                     <children>
                        <TextField fx:id="txtNome2" prefWidth="150.0" promptText="Nome" />
                        <TextField fx:id="txtCognome2" prefWidth="150.0" promptText="Cognome" />
                        <DatePicker fx:id="dpData2" prefWidth="150.0" />
                     </children>
                  </HBox>
                  <HBox layoutX="30.0" layoutY="169.0" spacing="10.0">
                     <children>
                        <TextField fx:id="txtNome3" prefWidth="150.0" promptText="Nome" />
                        <TextField fx:id="txtCognome3" prefWidth="150.0" promptText="Cognome" />
                        <DatePicker fx:id="dpData3" prefWidth="150.0" />
                     </children>
                  </HBox>
                  <HBox layoutX="30.0" layoutY="214.0" spacing="10.0">
                     <children>
                        <TextField fx:id="txtNome4" prefWidth="150.0" promptText="Nome" />
                        <TextField fx:id="txtCognome4" prefWidth="150.0" promptText="Cognome" />
                        <DatePicker fx:id="dpData4" prefWidth="150.0" />
                     </children>
                  </HBox>
                  <HBox layoutX="30.0" layoutY="259.0" spacing="10.0">
                     <children>
                        <TextField fx:id="txtNome5" prefWidth="150.0" promptText="Nome" />
                        <TextField fx:id="txtCognome5" prefWidth="150.0" promptText="Cognome" />
                        <DatePicker fx:id="dpData5" prefWidth="150.0" />
                     </children>
                  </HBox>
                  <HBox layoutX="30.0" layoutY="124.0" spacing="10.0" />
                  <Button fx:id="btnConferma" mnemonicParsing="false" onAction="#handleConferma" style="-fx-background-color: #27ae60;" text="CONFERMA">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#handleEsci" style="-fx-background-color: #e74c3c;" text="ESCI">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Button>
               </children>
               <padding> <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" /> </padding>
            </VBox>
         </children>
      </VBox>
   </children>
